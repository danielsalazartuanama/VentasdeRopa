                       aria-haspopup="true" aria-expanded="false">
                        <i class="fas fa-user"></i>
                        <span>@(Request.IsAuthenticated? User.Identity.GetUserName() : "Mi Cuenta")</span>
                    </a>
                    @if (Request.IsAuthenticated)
                    {
                        <ul id="ha" class="dropdown-menu" aria-labelledby="dropwdownMenu1">
                            <li>
                                <form action="~/account/logoff" method="post" id="logoutForm">
                                    @Html.AntiForgeryToken()
                                    <a class="dropdown-item" href="javascript:document.getElementById('logoutForm').submit()">Cerrar</a>
